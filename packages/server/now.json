{
  "version": 2,
  "name": "wga-server",
  "alias": ["server.authenticator.windowgadgets.io"],
  "builds": [{ "src": "src/index.ts", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "src/index.ts" }],
  "env": {
    "NODE_ENV": "production",
    "WGA_ADDRESS": "https://api.authenticator.windowgadgets.io",
    "WGA_SECRET_KEY": "@wga-secret-key"
  }
}
